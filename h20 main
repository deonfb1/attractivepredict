import h20
import lime
import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns
import numpy as np

mydata = pd.read_csv("")

#Enter "C" for classification and "R" for regression"
problem_type = "C" 

mydata['myresponse'] = mydata['attractive'] 
mydata.drop('attractive', axis=1, inplace=True) 
print(mydata.info())

#Create pandas dataframe
df = pd.Dataframe(mydata)

if problem_type = "C":
  table = pd.crosstab(df['myresponse'], columns=[1])
  print(table)

#Convert categorical variables
# mydata['xyz'] = mydata['xyz'].astype('category')
mydata['myresponse'] = mydata['myresponse'].astype('category')

hid_list = [[2], [2,2], [3,3], [4,4], [5,5], [2,2,2], [3,3,3], [4,4,4], [5,5,5]]
hyper_params = {'hidden': hid_list,
                'l1': [0, 0.001, 0.05]}

# Find the number of categorical predictors
numpredictors = mydata.shape[1] - 1
numfac = 0

# Loop through each predictor
for i in range(numpredictors):
  #Check if categorical
  if (mydata.iloc[:, i].dtype == 'object'):
    numfac = numfac + 1

rows = mydata.shape[0]
